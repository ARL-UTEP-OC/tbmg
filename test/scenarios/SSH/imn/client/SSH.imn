node n1 {
    type lanswitch
    network-config {
	hostname n1
	!
    }
    canvas c1
    iconcoords {455.0 186.0}
    labelcoords {455.0 210.0}
    interface-peer {e0 n2}
    interface-peer {e1 n3}
}

node n2 {
    type router
    model PC
    network-config {
	hostname n2
	!
	interface eth0
	 ip address 10.0.0.20/24
	!
    }
    canvas c1
    iconcoords {233.0 237.0}
    labelcoords {233.0 269.0}
    interface-peer {eth0 n1}
    custom-config {
	custom-config-id service:UserDefined
	custom-command UserDefined
	config {
	files=('SSHclient.sh', )
	cmdup=('sh SSHclient.sh', )
	starttime=1
	}
    }
    custom-config {
	custom-config-id service:UserDefined:SSHclient.sh
	custom-command SSHclient.sh
	config {
	file:///root/scenario/SSH/scripts/individual/SSHclient.sh
	}
    }
    services {DefaultRoute UserDefined}
}

node n3 {
    type rj45
    network-config {
	hostname eth0
	!
    }
    canvas c1
    iconcoords {645.0 250.0}
    labelcoords {645.0 282.0}
    interface-peer {0 n1}
}

link l1 {
    nodes {n2 n1}
    bandwidth 0
}

link l2 {
    nodes {n3 n1}
}

canvas c1 {
    name {Canvas1}
}

option global {
    interface_names no
    ip_addresses yes
    ipv6_addresses yes
    node_labels yes
    link_labels yes
    show_api no
    background_images no
    annotations yes
    grid yes
    traffic_start 0
}

option session {
}

